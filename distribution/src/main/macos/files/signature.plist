<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>rules</key>
    <dict>
        <!-- Recursively include 'MacOS' and 'Resources' dirs -->
        <key>^MacOS/</key>
        <true/>
        <key>^Resources/</key>
        <true/>
        <!-- Omit a file -->
        <key>^Resources/Java</key>
        <dict>
            <key>omit</key>
            <true/>
            <key>weight</key>
            <real>1100</real>
        </dict>
	<key>^Info\.plist$</key>
	<dict>
	    <key>omit</key>
            <true/>
            <key>weight</key>
            <real>20</real>
	</dict>
        <key>^version.plist$</key>
        <true/>
    </dict>
</dict>
</plist>
