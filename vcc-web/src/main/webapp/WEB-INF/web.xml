<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <display-name>VCC Web Interface</display-name>
    <servlet>
        <servlet-name>Greeting</servlet-name>
        <servlet-class>vcc.dnd4e.web.servlet.GreetingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Capture</servlet-name>
        <servlet-class>vcc.dnd4e.web.servlet.CaptureServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlayerView</servlet-name>
        <servlet-class>vcc.dnd4e.web.servlet.PlayerViewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Greeting</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Capture</servlet-name>
        <url-pattern>/capture</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlayerView</servlet-name>
        <url-pattern>/player-view</url-pattern>
    </servlet-mapping>
</web-app>
